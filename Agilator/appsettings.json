{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=Agilator;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Agilator": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "JWTSettings": {
    "securityKey": "SecretAgilatorKey",
    "validIssuer": "AgilatorAPI",
    "validAudience": "https://localhost:44350",
    "expiryInMinutes": 43200
  },
  "AllowedHosts": "*"
}
